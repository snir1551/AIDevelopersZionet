{
  "schema": 1,
  "description": "Creates release notes based on commits",
  "execution_settings": {
    "default": {
      "max_tokens": 1,
      "temperature": 0.6,
      "top_p": 0.0,
      "presence_penalty": 0.0,
      "frequency_penalty": 0.0
    }
  },
  "input_variables": [
    {
      "name": "version",
      "description": "Release version",
      "default": "",
      "is_required": true
    },
    {
      "name": "date",
      "description": "Optional release date",
      "default": "",
      "is_required": false
    },
    {
      "name": "commitInfo",
      "description": "List of commits to analyze",
      "default": "",
      "is_required": true
    }
  ]
}